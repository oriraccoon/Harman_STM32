#include "FND.h"

typedef struct
{
	GPIO_TypeDef *GPIOx;
	uint32_t pinNum;
	uint32_t *data;
}Handler_FND_TypeDef;


void FND_Init(Handler_FND_TypeDef *hfnd, GPIO_TypeDef *GPIOx, uint32_t pinNum)
{
	hfnd->GPIOx = GPIOx;
	hfnd->pinNum = pinNum;
	hfnd->data = 0;

	GPIO_init(hfnd->GPIOx, hfnd->pinNum, OUTPUT);
}
