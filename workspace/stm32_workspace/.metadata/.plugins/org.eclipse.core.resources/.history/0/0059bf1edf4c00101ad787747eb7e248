#ifndef DRIVER_FND_FND_H_
#define DRIVER_FND_FND_H_

typedef struct
{
	GPIO_TypeDef *GPIOx;
	uint32_t pinNum;
	uint32_t data;
}Handler_FND_TypeDef;

#include <stdint.h>
#include "SystemClock.h"
#include "GPIO.h"

void FND_Init(Handler_FND_TypeDef *hfnd, GPIO_TypeDef *GPIOx, uint32_t pinNum);
void FND_DATA_Init();
void FND_Write(uint32_t data);

#endif /* DRIVER_FND_FND_H_ */
